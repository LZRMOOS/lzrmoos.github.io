{"ast":null,"code":"// Import Swiper core and required modules\nimport { Keyboard, Navigation, Autoplay, Pagination } from \"swiper\"; // Import Swiper Vue.js components\n\nimport { Swiper, SwiperSlide } from \"swiper/vue\"; // Import Swiper styles\n\nimport \"swiper/css\";\nimport \"swiper/css/pagination\";\nimport \"swiper/css/navigation\"; // SwiperCore.use([Navigation]);\n\nexport default {\n  name: 'HelloWorld',\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n\n  setup() {\n    return {\n      modules: [Keyboard, Navigation, Autoplay, Pagination]\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiCA;AACA,SAASA,QAAT,EAAmBC,UAAnB,EAA+BC,QAA/B,EAAyCC,UAAzC,QAA2D,QAA3D,C,CAEA;;AACA,SAASC,MAAT,EAAiBC,WAAjB,QAAoC,YAApC,C,CAEA;;AACA,OAAO,YAAP;AACA,OAAO,uBAAP;AACA,OAAO,uBAAP,C,CAEA;;AACA,eAAe;EACbC,IAAI,EAAE,YADO;EAEbC,UAAU,EAAE;IACVH,MADU;IAEVC;EAFU,CAFC;;EAMbG,KAAK,GAAG;IACN,OAAO;MACLC,OAAO,EAAE,CAACT,QAAD,EAAWC,UAAX,EAAuBC,QAAvB,EAAiCC,UAAjC;IADJ,CAAP;EAGF;;AAVa,CAAf","names":["Keyboard","Navigation","Autoplay","Pagination","Swiper","SwiperSlide","name","components","setup","modules"],"sourceRoot":"","sources":["/Users/weid/src/vuejstest/src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div class=\"swiper-container\">\n    <swiper\n    :parallax=\"true\"\n    :speed=\"1000\"\n    :slides-per-view=\"1\"\n    :autoplay=\"{\n      delay: 5000,\n      disableOnInteraction: true,\n    }\" \n    :navigation=\"true\"\n    :modules=\"modules\"\n    :calculate-height=\"true\"\n    :keyboard=\"{\n      enabled: true,\n    }\"\n    :pagination=\"{\n      clickable: true,\n      // type: 'progressbar'\n    }\"\n    class=\"mySwiper\"\n    >\n      <swiper-slide>\n        <img src=\"https://i.imgur.com/ZWyJYJt.jpg\" alt=\"Test1\" />\n      </swiper-slide>\n      <swiper-slide>\n        <img src=\"https://lzrmoos.com/photography/images/fulls/DSC_7380.jpg\" alt=\"Test1\" />\n      </swiper-slide>\n    </swiper>\n  </div>\n</template>\n\n<script>\n// Import Swiper core and required modules\nimport { Keyboard, Navigation, Autoplay, Pagination } from \"swiper\";\n\n// Import Swiper Vue.js components\nimport { Swiper, SwiperSlide } from \"swiper/vue\";\n\n// Import Swiper styles\nimport \"swiper/css\";\nimport \"swiper/css/pagination\";\nimport \"swiper/css/navigation\";\n\n// SwiperCore.use([Navigation]);\nexport default {\n  name: 'HelloWorld',\n  components: {\n    Swiper,\n    SwiperSlide,\n  },\n  setup() {\n    return {\n      modules: [Keyboard, Navigation, Autoplay, Pagination],\n    };\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style>\n.swiper-container {\n  width:auto !important;\n  height:auto;\n  object-fit:cover;\n  object-position:center;\n}\n.swiper-slide {\n  /* width:auto; */\n  height:auto;\n}\n.swiper-container img {\n  width:100%;\n  height:auto;\n}\n.swiper-pagination-progressbar {\n  background-color: rgb(158, 158, 158);\n  opacity: 75%;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}